{
  "functions": [
    {
      "source": ".output/server",
      "runtime": "nodejs16",
      "codebase": "ugcserver"
    }
  ],
  "hosting": {
    "site": "mediaplus-ugc-platform-nuxt3-demo",
    "public": ".output/public",
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{ "source": "**", "function": "server" }]
  }
}
